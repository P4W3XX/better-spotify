{
  "builds": [
    {
      "src": "backend/betterspotify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "buildCommand": "bash setup.sh"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/betterspotify/wsgi.py"
    },
    {
      "src": "/api/get-cookie",
      "dest": "app/api/get-cookie/route.ts",
      "methods": ["GET"],
      "runtime": "nodejs18.x"
    },
    {
      "src": "/api/set-cookie",
      "dest": "app/api/set-cookie/route.ts",
      "methods": ["GET"],
      "runtime": "nodejs18.x"
    },
    {
      "src": "/api/test",
      "dest": "app/api/test/route.ts",
      "methods": ["GET"],
      "runtime": "nodejs18.x"
    },
    {
      "src": "/static/(.*)",
      "dest": "backend/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
