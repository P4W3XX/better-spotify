{
  "builds": [
    {
      "src": "backend/betterspotify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11",
        "buildCommand": "bash setup.sh",
        "ignoreCommand": "if [ \"$VERCEL_GIT_BRANCH\" = \"api\" ]; then exit 0; fi"
      }
    },
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/betterspotify/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "backend/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
